<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<Scene xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lv.venta.SecondaryController">
    <AnchorPane prefHeight="486.0" prefWidth="908.0">
      <children>
         <ImageView fitHeight="523.0" fitWidth="959.0" layoutX="-6.0" layoutY="-37.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@background2.gif" />
            </image>
         </ImageView>
         <Button fx:id="startButton" layoutX="363.0" layoutY="199.0" mnemonicParsing="false" onAction="#openRegister" prefHeight="65.0" prefWidth="199.0" styleClass="Button" stylesheets="@application.css" text="START" />
         <Button fx:id="optionsButton" layoutX="385.0" layoutY="282.0" mnemonicParsing="false" onAction="#openOptions" prefHeight="45.0" prefWidth="155.0" styleClass="Button" stylesheets="@application.css" text="OPTIONS" />
         <Button fx:id="ExitButton" layoutX="384.0" layoutY="334.0" mnemonicParsing="false" onAction="#exitOut" prefHeight="45.0" prefWidth="155.0" styleClass="Button" stylesheets="@application.css" text="EXIT" />
      </children></AnchorPane>
</Scene>
